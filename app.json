{
  "expo": {
    "name": "JOYNER",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joyner.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo.png",
      "name": "JOYNER",
      "shortName": "JOYNER",
      "description": "내 손안의 AI 스케줄링 비서, JOYNER",
      "lang": "ko",
      "startUrl": "/",
      "display": "standalone",
      "backgroundColor": "#ffffff",
      "themeColor": "#ffffff",
      "meta": {
        "apple": {
          "mobileWebAppCapable": "yes"
        },
        "og:site_name": "JOYNER",
        "og:title": "JOYNER - AI 스케줄링 비서",
        "og:description": "내 손안의 AI 스케줄링 비서, JOYNER를 지금 만나보세요.",
        "og:type": "website",
        "og:image": "https://www.joyner.co.kr/logo.png",
        "og:locale": "ko_KR"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7C6EE6"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "902b3bac-e14b-4e05-a200-7c64110cd621"
      }
    }
  }
}